<script>
  onResizeSetScroll();
  window.addEventListener('resize', onResizeSetScroll);
  /* Check Hero Banner Height and set fixed menu starting point */
  function onResizeSetScroll(){
      if (window.innerWidth < 767) {        
          var imgHero = $(".ratio_wrapper.res-100"); 
          var heightHero = imgHero.outerHeight()-50;
      } else if(window.innerWidth < 990) {
         
         var imgHero = $(".ratio_wrapper.res-single"); 
          var heightHero = imgHero.outerHeight()-50;        
      }else {
          var imgHero = $(".ratio_wrapper"); 
          var heightHero = imgHero.outerHeight()-50;
      } 
      $(window).scroll(function () {
        if($(window).scrollTop() > heightHero) {
            $(".eye-complexion-tabs").addClass('sticky'); 
            $(".eye-navSection").addClass('extraPadding'); 
          } else {
            $(".eye-complexion-tabs").removeClass('sticky');
           $(".eye-navSection").removeClass('extraPadding'); 
          }
      });
  }

  
$(document).ready(function () {
    $(document).on("scroll", onScroll);
    //smoothscroll
    $('a[href^="#"]').on('click', function (e) {
        $(document).off("scroll");
        
        $('#onepage-nav a').each(function () {
          //  $(this).removeClass('active');
        })
      //  $(this).addClass('active');
       
        var target = this.hash,
            menu = target;
        $target = $(target);
      
         if (window.innerWidth < 990) {  
              $('html, body').stop().animate({
                  'scrollTop': $target.offset().top-170
              }, 250, 'swing', function () {
                  window.location.hash = target;
                  $(document).on("scroll", onScroll);
              });
         } else {
            $('html, body').stop().animate({
                  'scrollTop': $target.offset().top-220
              }, 250, 'swing', function () {
                  window.location.hash = target;
                  $(document).on("scroll", onScroll);
              });
           
         }

     if (window.innerWidth < 990) {
              if(target == "#mascara") {
             setTimeout(function(){  
                       $('html, body').stop().animate({
                                'scrollTop': $target.offset().top-130
                            }, 250, 'swing', function () {
                                window.location.hash = target;
                                $(document).on("scroll", onScroll);
                            });
                },250);
                         
             }
    }
           
    });
});

  


  
function onScroll(event){
    var scrollPos = $(document).scrollTop();
    var mobileHeroHeight = $("#mobileBanner").outerHeight();

 
    if (window.innerWidth < 990) {   
        if($(document).scrollTop() > mobileHeroHeight+100 )  {
            setTimeout(function(){  
              $(".content_link.active")[0].scrollIntoView({behavior: "smooth", block: "nearest", inline: "center"});
          },200);
        } else {
          $(".eye-complexion-tabs").removeClass("sticky");
          $(".eye-navSection").removeClass("extraPadding"); 
        }
     }
          
           
    $('#onepage-nav a').each(function () {
        var currLink = $(this);
        var refElement = $(currLink.attr("href"));
        if (refElement.position().top - 300 <= scrollPos && refElement.position().top + refElement.height() > scrollPos) {
            $('#onepage-nav a').removeClass("active");
            currLink.addClass("active");

          if (window.innerWidth < 990) { 
              $(".effect").width($(this).children("span").css("width"));
            setTimeout(function(){
                var anchorOffset  = $(".content_link.active").position();
                 $(".effect").css("left", anchorOffset.left + parseInt(115));   
            },200);

            
            }  else {
          
                $(".effect").width($(this).children("span").css("width"));
                var anchorOffset  = $(this).children("span").offset();
                var screenRes = $(window).width();
                var navWrapRes = $("#onepage-nav").width();
                
                var calcLeftSpan = (screenRes - navWrapRes) / 2;
        
                 $(".effect").css("left", anchorOffset.left - parseInt(calcLeftSpan) + parseInt(100));        
            }         
           
        }
        else{
          //  currLink.removeClass("active");
        }
    });
          
     
}

</script>


