<script>
    $(document).ready(function(){
        var fadeIn = 'animate__animated animate__fadeInRight';
        var fadeOut = 'animate__animated animate__fadeOutLeft';
        $( ".product-card-tabs li:first-child .subnav_content_link" ).addClass('active');

        $( ".product-card-tabs .subnav_content_link" ).on( "click", function(e) {
            e.preventDefault();
            $(this).parent().parent().find('.subnav_content_link').removeClass('active');
            $(this).addClass('active');
            $(this).parent().parent().next().find('.content_div').hide();
            $(this).parent().parent().next().find('#content_div'+$(this).attr('data-id')).fadeIn(300);
        });

    });
</script>

