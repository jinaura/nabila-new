{% assign logo_url = section.settings.logo %}
{% assign logo_size = section.settings.logo_size %}
{% assign main_menu = section.settings.main_menu %}

<style>
    header.custom.white-bg{ background: #fff;}

    header.white-bg .logo svg, header.white-bg .toolBar, header.white-bg .mainNav nav a,
    header.white-bg .mobileToolBar {
        filter: invert(0) brightness(0) !important;
    }

    @media only screen and (min-width: 990px) {
        header.custom .toolBar svg:hover{
            fill:#d8d8d8;
        }
    }

    header.custom .logo{
        max-width: {{ logo_size }}px;
    }
    header.custom .mainNav,
    header.custom .toolBar,
    header.custom .mobileToolBar {
        width: calc( (100% - {{ logo_size }}px) /2);
    }
    .form-search .field {
        border: none;
        font-size: 20px !important;
        font-weight: normal;
        font-stretch: normal;
        font-style: normal;
        line-height: 32px;
        letter-spacing: normal;
        color: #919191;
        background: transparent;
        border-bottom: 1px solid #919191;
        padding-left: 0;
        padding-bottom: 10px;
        padding-right: 0;
        padding-top: 10px;
        -webkit-appearance: none;
        border-radius: 0;
    }
    .isNavwrapperOpen .form-search {
        position: relative;
        width: 96%;
    }
    .isNavwrapperOpen .form-search .search-btn {
        width: 21px;
        height: 21px;
        position: absolute;
        right: 0;
        top: 0;
        background: transparent;
        border: none;
    }
    .isNavwrapperOpen .form-search .search-btn span {
        /* background: url({{ 'gnav-search.png' | asset_url }}) no-repeat; */
        width: 21px;
        height: 21px;
        top: 0;
        position: absolute;
        right: 0;
    }
    .SearchSidebarMegaMenu {
        display: none;
    }
    .SearchSidebarMegaMenu.show-search {
        display: block;
    }

    /* Start New css */

    .s .form-search {
        position: relative;
    }
    .s .form-search .search-btn {
        width: 21px;
        height: 21px;
        position: absolute;
        right: 0;
        top: 14px;
        background: transparent;
        border: none;
    }
    .s .form-search .search-btn span {
        /* background: url({{ 'gnav-search.png' | asset_url }}) no-repeat; */
        width: 21px;
        height: 21px;
        top: 0;
        position: absolute;
        right: 0;
    }
    .s .form-search .field {
        width: 100%;
    }
    [data-search-panel] {
        position: fixed;
        width: 100%;
        height: 100%;
        top: 0;
        right: 0;
        display: flex;
        z-index: -100;
    }
    .search-panel {
       background: #fff;
    width: 100%;
    position: relative;
    left: 0;
    z-index: 2;
    flex-basis: auto;
    height: 0;
    overflow: hidden;
    box-sizing: border-box;
    top: 120px
    }
    .searchBox {
        padding: 0 40px;
      max-width:1095px;
      margin:0 auto;
    }
    .SearchOpened {
        z-index: 9;
    }
    .searchBox {
        padding: 0 40px;
    }
    .navPanel.search-panel {
        transition: all 0.7s cubic-bezier(.32, .24, .15, 1);
    }
    .isActiveSearch {
        transition: all 1s cubic-bezier(.32, .24, .15, 1);
        height: 75%;
        padding-top: 25px;
        padding-bottom: 25px;
    }
    #mobile-search-toggle-icon {
        display:none;
  	}
    .mobile-search-wrap {
        visibility: hidden;
        opacity: 0;
        transition: opacity .3s cubic-bezier(.215, .61, .355, 1);
    }
    .show-mobile-search {
        visibility: visible;
        opacity: 1;
       transition: opacity .3s cubic-bezier(.215, .61, .355, 1);
    }
    .fixed-box .fixed-header {
        position: relative;
        top: 0;
        opacity: 0;
    }
    .overlayBgSearch{
        background: rgba(0,0,0,.4);
        position: fixed;
        width: 100%;
        height: 100%;
        top: 0;
        left: 0;
        transition: opacity .5s;
        opacity: 0;
        z-index: -1;
    }
    .overlayBgSearch.isActive{
        opacity: 1;
    }
    #SearchContent .shutter{
        width: 100%;
    }
    #SearchContent .product__item{
        margin-bottom: 20px;
    }
    @media (min-width:1025px) {
        .mobile-search-wrap {
            display:none;
        }
    }
    @media (max-width:1024px) {
        #search-toggle-icon {
          display:none;
        }
        #mobile-search-toggle-icon {
          display:block;
        }
        .mobile-search-wrap {
          background: #fff;
          width: 100%;
          position: fixed;
          left: 0;
          right: 0;
          top: 100px;
          z-index: 9999999999;
        }
        .searchBox {
          padding: 0 20px;
          padding-bottom: 15px;
        }
        .fade-icon {
          opacity: 0.4;
        }
    }
    @media only screen and (min-width:768px)  and (max-width: 1024px){
        .mobile-search-wrap {
          top: 113px;
        }
    }

    {% if template == 'index' and section.settings.is_transparent and localization.country.iso_code != 'GB' %}
        header.custom{
            transition: background 0.2s;
        }
        header.opt-transparent,
        header.opt-transparent.relative:before{
          background:transparent;
        }
        header.opt-light .mainNav nav a{
          color:#000;
        }
        header.opt-light.relative .mainNav nav a{
            color: var(--secondary-color);
        }

        {% if section.settings.is_white_nav %}
            header.opt-transparent .logo svg,
            header.opt-transparent .toolBar,
            header.opt-light .mainNav nav a,
            header.opt-transparent .mobileToolBar{
              filter: invert(1) brightness(100);
            }
        {% endif %}

        {% if section.settings.is_transparent %}
            .heroBanner{
              margin-top:-80px;
            }
            @media (max-width:767px){
             .heroBanner{
                margin-top:-100px;
              }
                html.sticky-below-fold body {
                    margin-top: 0!important;
                }
            }
            .hero-section-desktop .heroBanner .media .res-single{
              padding-top: calc(37.01% + 135px);
            }
        {% endif %}
    {% endif %}

    {% if template == 'index' and section.settings.is_transparent_uk and localization.country.iso_code == 'GB' %}
        header.custom{
            transition: background 0.2s;
        }
        header.opt-transparent,
        header.opt-transparent.relative:before{
            background:transparent;
        }
        header.opt-light .mainNav nav a{
            color:#000;
        }
        header.opt-light.relative .mainNav nav a{
            color: var(--secondary-color);
        }

        {% if section.settings.is_white_nav_uk %}
            header.opt-transparent .logo svg,
            header.opt-transparent .toolBar,
            header.opt-light .mainNav nav a,
            header.opt-transparent .mobileToolBar{
                filter: invert(1) brightness(100);
            }
        {% endif %}

        {% if section.settings.is_transparent_uk %}
            .heroBanner{
                margin-top:-80px;
            }
            @media (max-width:767px){
                .heroBanner{
                    margin-top:-100px;
                }
                html.sticky-below-fold body {
                    margin-top: 0!important;
                }
            }
            .hero-section-desktop .heroBanner .media .res-single{
                padding-top: calc(37.01% + 135px);
            }
        {% endif %}
    {% endif %}

    {% if template == 'page.eye-collection' and section.settings.is_transparent_eye_landing %}
    header.custom{
        transition: background 0.2s;
    }
    header.opt-transparent,
    header.opt-transparent.relative:before{
        background:transparent;
    }
    header.opt-light .mainNav nav a{
        color:#fff;
    }
    header.opt-light.relative .mainNav nav a{
        color: var(--secondary-color);
    }
    header.opt-light .logo svg,
    header.opt-light .toolBar,
    header.opt-light .mobileToolBar{
        filter: invert(1) brightness(100);
    }

    {% if section.settings.is_light_eye_landing %}
    .heroBanner{
        margin-top:-80px;
    }
    @media (max-width:767px){
        .heroBanner{
            margin-top:-60px;
        }
    }
    .hero-section-desktop .heroBanner .media .res-single{
        padding-top: calc(37.01% + 135px);
    }
    {% endif %}

{% endif %}


{% if template == 'collection' and collection.handle == 'holiday-gift-guide' and section.settings.is_transparent_holiday_coll %}
  header.custom{
    transition: background 0.2s;
  }
  header.opt-transparent,
  header.opt-transparent.relative:before{
    background:transparent;
  }
  header.opt-light .mainNav nav a{
    color:#fff;
  }
  header.opt-light.relative .mainNav nav a{
    color: var(--secondary-color);
  }
  header.opt-light .logo svg,
  header.opt-light .toolBar,
  header.opt-light .mobileToolBar{
    filter: invert(1) brightness(100);
  }

  {% if section.settings.is_light_holiday_coll %}
    .heroBanner{
      margin-top:-80px;
    }
    @media (max-width:767px){
      .heroBanner{
        margin-top:-60px;
      }
    }
    .hero-section-desktop .heroBanner .media .res-single{
      padding-top: calc(37.01% + 135px);
    }
  {% endif %}

{% endif %}

{% if template == 'page.shade-finder' and section.settings.is_transparent_sf %}
  header.custom{
    transition: background 0.2s;
  }
  header.opt-transparent,
  header.opt-transparent.relative:before{
    {% comment %} background:transparent; {% endcomment %}
    background: linear-gradient(180deg, rgba(86, 85, 85, 0.20) 0%, rgba(51, 51, 51, 0.20) 0.52%, rgba(217, 217, 217, 0.00) 100%);
  }
  header.opt-light .mainNav nav a{
    color:#fff;
  }
  header.opt-light.relative .mainNav nav a{
    color: var(--secondary-color);
  }
  header.opt-light .logo svg,
  header.opt-light .toolBar,
  header.opt-light .mobileToolBar{
    filter: invert(1) brightness(100);
  }

  {% if section.settings.is_light_sf %}
    .main .container{
      margin-top:-80px;
    }
    @media (max-width:767px){
      .main .container{
        margin-top:-60px;
      }
    }
    .hero-section-desktop .heroBanner .media .res-single{
      padding-top: calc(37.01% + 135px);
    }
  {% endif %}
