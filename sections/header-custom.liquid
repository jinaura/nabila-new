<header class="custom  {{ is_transparent }} {{ is_light }} {% if template == 'page.eye-collection' and  section.settings.is_transparent_eye_landing or section.settings.is_light_eye_landing %} eye-collection-header {% endif %}{% if template == 'collection' and collection.handle == 'holiday-gift-guide' and  section.settings.is_transparent_holiday_coll or section.settings.is_light_holiday_coll %} holiday-collection-header {% endif %}{% if template == 'page.shade-finder' and  section.settings.is_transparent_sf or section.settings.is_light_sf %} sf-page-header {% endif %}" data-section-id="{{ section.id }}" data-section-type="header">
    {%- render 'react-i18n' translation_key: 'global' -%}
    {%- render 'react-i18n' translation_key: 'accessibility' -%}
    <div class="container">
        <div class="mobileToolBar" id="mobileToolBar">
          <a href="javascript:void(0)"><span class="mobileToggle"></span></a>
            {% liquid
                if customer
                    assign account_url = '/account'
                else
                    assign account_url ='/account/login?return_url=%2Faccount'
                endif
            %}
            <a href="{{ account_url }}" class='mobileAccount'>{%- include 'svg-icons' with 'user' -%}</a>
        </div>
        <div class="mainNav hidden-sm hidden-xs" id="main_menu">
            <nav>
                <ul>

                    {% for link in linklists[main_menu].links %}
                        <li>
                            <a href="{{ link.url }}"
                               class="{{link.title | replace:" ","-" | downcase }} {% if link.current %}selected{% endif %}"
                               {% if link.current %}aria-current="page"{% endif %}>{{ link.title }}
                            </a>
                        </li>
                    {% endfor %}

                </ul>
            </nav>
        </div>

        <div class="logo">
            {% if logo_url != blank %}
                <a href="/"><img src="{{ logo_url | img_url:"500x" }}" alt="{{ logo_url.alt_text }}"></a>
            {% else %}
                <a href="/">{%- include 'svg-icons' with 'logo' -%}</a>
            {% endif %}
        </div>

        <div class="toolBar">
            <a href="{{ account_url }}" class='hidden-sm hidden-xs account-icon'>{%- include 'svg-icons' with 'user' -%}</a>
            <button id="search-toggle-icon" class="search-icon" aria-label="Search">{%- include 'svg-icons' with 'search' -%}</button>
            <button id="mobile-search-toggle-icon" class="search-icon" aria-label="Search">{%- include 'svg-icons' with 'search' -%}</button>
            <!-- <a href="/cart"><span class="cart-count">{{cart.item_count}}</span></a> -->
            <button class="cart_button" aria-label="My Cart"><span class="cartCount">{% include 'svg-icons' with 'cart-new'%} <span>{{ cart.item_count }}</span></span></button>

        </div>
    </div>
</header>

