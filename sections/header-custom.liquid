{% endif %}

<script type="text/javascript">
  $(document).ready(function () {

      //jQuery('a#search-toggle-icon').on('mouseover', function(e){
    jQuery('button#search-toggle-icon').on('click', function(e){
      e.preventDefault();
      /// if popup already open ////
      if(jQuery('.search-panel').hasClass('isActiveSearch'))
      {
          jQuery('.search-panel').removeClass('isActiveSearch');
          jQuery('.overlayBgSearch').removeClass('isActive');
          jQuery(this).removeClass('isActive');
          jQuery('body, html').removeClass('overflow-hidden');
          setTimeout(function(){
              jQuery('header.custom').removeClass('white-bg');
              jQuery('.hideSearch').removeClass('SearchOpened');
              //}, 500);
          }, 500);
      }else {
          //setTimeout(function () {
              jQuery('.search-panel').addClass('isActiveSearch');
              jQuery('.overlayBgSearch').addClass('isActive');
              jQuery('.hideSearch').addClass('SearchOpened');
              jQuery('body, html').addClass('overflow-hidden');
              jQuery('header.custom').addClass('white-bg');
              jQuery('.search-panel input').focus();
          //}, 100);
      }
    });

   //jQuery('.search-panel .closeMenu, .overlayBgSearch').on('click', function(e){
   $('.overlayBgSearch,.gossip,.shop,.shade-finder,.virtual-try-on,.editorial.cart_button').on('mouseover', function(e){
      e.preventDefault();

      //// hide scroll when mouse hover on main navigation ///
        if($(".navPanel").hasClass("isActive") && (jQuery(this).hasClass('virtual-try-on') || jQuery(this).hasClass('shade-finder')) )
        {
            jQuery('body, html').addClass('overflow-hidden');
        }else{
            jQuery('body, html').removeClass('overflow-hidden');
        }

      //setTimeout(function(){
          jQuery('.search-panel').removeClass('isActiveSearch');
          jQuery('.overlayBgSearch').removeClass('isActive');
          jQuery(this).removeClass('isActive');
          //jQuery('body, html').removeClass('overflow-hidden');
          setTimeout(function(){
            jQuery('header.custom').removeClass('white-bg');
            jQuery('.hideSearch').removeClass('SearchOpened');
          //}, 500);
      }, 500);
    });




    // Mobile Search

    jQuery('#mobile-search-toggle-icon').on('click', function(e){
      e.preventDefault();
      jQuery(this).toggleClass('fade-icon');
      jQuery('.mobile-search-wrap').toggleClass('show-mobile-search');
      jQuery('.overlayBgSearch').toggleClass('isActive');
      jQuery('.mobile-search-wrap input').toggleClass('input-focus');
      jQuery('.mobile-search-wrap input').focus();
      jQuery('body, html').toggleClass('overflow-hidden');
      jQuery(this).blur();
    });

    jQuery('.overlayBgSearch').on('click', function(){
      jQuery(this).removeClass('isActive');
      jQuery('.mobile-search-wrap').removeClass('show-mobile-search');
      jQuery('#mobile-search-toggle-icon').removeClass('fade-icon');
      jQuery('body, html').removeClass('overflow-hidden');
    });

    var getHeaderHeight;
    var getHeaderMobileHeight;
    var getWidowWidth;
    var getHeroBottomPercentage;

    windowAndHero();
    window.addEventListener('resize', windowAndHero);

    function windowAndHero () {
      getHeaderHeight  = $(".hero-section-desktop").outerHeight();
      getHeaderMobileHeight = $(".hero-section-mobile").outerHeight();
      getWidowWidth = $(document).width();
      if($('.template-page--shade-finder').length) {
        if (getWidowWidth > 1400) {
          getHeroBottomPercentage =  $("header").outerHeight() / 100 * 70;
        } else if (getWidowWidth < 1399){
          getHeroBottomPercentage =  $("header").outerHeight() / 100 * 80;
        }
      } else {
        if (getWidowWidth > 1400) {
          getHeroBottomPercentage =  getHeaderHeight / 100 * 70;
        } else if (getWidowWidth < 1399 && getWidowWidth > 768){
          getHeroBottomPercentage =  getHeaderHeight / 100 * 80;
        } else if(getWidowWidth < 767) {
          getHeroBottomPercentage =  getHeaderMobileHeight / 100 * 80;
        }
      }
    }

    {% if template == 'index' or template == 'page.eye-collection' or template == 'page.shade-finder' or template == 'collection' and collection.handle == 'holiday-gift-guide' %}
      $(window).on('scroll', ()=>{
        {%if template == 'index'%}
          if(window.scrollY > 60){
            $('header.custom').removeClass('opt-transparent opt-light');
          }else{
            if(!$('.mobileWrapper').hasClass('isActive')){
              {% if section.settings.is_transparent and localization.country.iso_code != 'GB' %}
                  $('header.custom').addClass('opt-transparent');
              {% elsif section.settings.is_transparent_uk and localization.country.iso_code == 'GB' %}
                  $('header.custom').addClass('opt-transparent');
              {% endif %}
              {% if section.settings.is_light and localization.country.iso_code != 'GB' %}
                $('header.custom').addClass('opt-light');
              {% elsif section.settings.is_light_uk and localization.country.iso_code == 'GB' %}
                $('header.custom').addClass('opt-light');
              {% endif %}
            }
          }
        {%endif%}
      });

      {% if section.settings.is_transparent or section.settings.is_transparent_uk or section.settings.is_transparent_eye_landing or section.settings.is_transparent_holiday_coll or section.settings.is_transparent_sf %}

        if($(window).width() > 767){
          $('header.custom').hover(function(){
            if(!document.querySelector('header').classList.contains('no-scroll-change')){
            if(window.scrollY < 60){
              $(this).removeClass('opt-transparent opt-light');
            }
            }
            }, function(){
              if(!document.querySelector('header').classList.contains('no-scroll-change')){
              if(window.scrollY < 60){
                $(this).addClass('opt-transparent opt-light');
              }else{
                $(this).removeClass('opt-transparent opt-light');
              }
            }
          });
        }

        $('[data-navigation-panel]').hover(function(){
          if(!document.querySelector('header').classList.contains('no-scroll-change')){
            if(window.scrollY < 60){
              $('header.custom').removeClass('opt-transparent opt-light');
            }
          }
          }, function(){
            if(!document.querySelector('header').classList.contains('no-scroll-change')){
            if(window.scrollY < 60){
              if(!$(this).hasClass('isNavwrapperOpen')) {
                $('header.custom').addClass('opt-transparent opt-light');
              }
            }else{
                $('header.custom').removeClass('opt-transparent opt-light');
            }
          }
          }
      );

        $('.mobileToggle, #mobile-search-toggle-icon, .overlayBgSearch').on('click', function(){
          {%if template == 'index'%}
            if(window.scrollY < 60) {
              $('header.custom').toggleClass('opt-transparent opt-light');
            }
          {%elsif template == 'collection' and collection.handle == 'holiday-gift-guide'%}
            if(window.scrollY < getHeroBottomPercentage && $(window).width() < 950 &&!$('.holiday-plp').hasClass('no_scroll_quickshop')) {
              $('header.custom').toggleClass('opt-transparent opt-light');
              $("header.custom").toggleClass('holiday-collection-header');
            }
          {%elsif template == 'page.eye-collection'%}
            if(window.scrollY < getHeroBottomPercentage) {
              $('header.custom').toggleClass('opt-transparent opt-light');
              $("header.custom").toggleClass('eye-collection-header');
            }
          {%elsif template == 'page.shade-finder'%}
            if(window.scrollY < getHeroBottomPercentage) {
              if(!document.querySelector('header').classList.contains('no-scroll-change')){
                $('header.custom').toggleClass('opt-transparent opt-light');
                $("header.custom").toggleClass('sf-page-header');
              }
            }
          {%endif%}
        });
      {% endif %}

    {% endif %}

    {% if template == 'page.eye-collection' and section.settings.is_transparent_eye_landing or section.settings.is_light_eye_landing%}
      $(window).on('scroll', ()=>{
        if(window.scrollY > getHeroBottomPercentage){
          $('header.custom').removeClass('opt-transparent opt-light');
          $('header.custom').removeClass('eye-collection-header');
        } else {
          if(!$('.mobileWrapper').hasClass('isActive')) {
            $('header.custom').addClass('eye-collection-header');
            {% if section.settings.is_transparent_eye_landing %}
              $('header.custom').addClass('opt-transparent');
            {% endif %}
            {% if section.settings.is_light_eye_landing %}
              $('header.custom').addClass('opt-light');
            {% endif %}
          }
        }
      });

      $(".navPanel").hover(
        function() {
            $("header.custom").removeClass('eye-collection-header');
        }, function() {
          if(window.scrollY < getHeroBottomPercentage){
            $('header.custom').addClass('eye-collection-header');
          }
      });
    {% endif %}

    {% if template == 'collection' and collection.handle == 'holiday-gift-guide' and section.settings.is_transparent_holiday_coll or section.settings.is_light_holiday_coll %}
      $(window).on('scroll', ()=>{
        if(window.scrollY > getHeroBottomPercentage){
          $('header.custom').removeClass('opt-transparent opt-light');
          $('header.custom').removeClass('holiday-collection-header');
        } else {
          if(!$('.mobileWrapper').hasClass('isActive') && !$('.holiday-plp').hasClass('no_scroll_quickshop')) {
            $('header.custom').addClass('holiday-collection-header');
            {% if section.settings.is_transparent_holiday_coll %}
              $('header.custom').addClass('opt-transparent');
            {% endif %}
            {% if section.settings.is_light_holiday_coll %}
              $('header.custom').addClass('opt-light');
            {% endif %}
          }
        }
      });

      $(document).on('click', '.holiday-coll-btn.buy_btn', function(el){
        if($(window).width() < 950) {
          $('.holiday-plp').addClass('no_scroll_quickshop');
        }
      });

      $(document).on('click', '.holiday-plp .shutter .close', function(el){
        if($(window).width() < 950) {
          $('.holiday-plp').removeClass('no_scroll_quickshop');
        }
      });

      $(".navPanel").hover(
        function() {
            $("header.custom").removeClass('holiday-collection-header');
        }, function() {
          if(window.scrollY < getHeroBottomPercentage){
            $('header.custom').addClass('holiday-collection-header');
          }
        }
      );
    {% endif %}

    {% if template == 'page.shade-finder' and section.settings.is_transparent_sf or section.settings.is_light_sf %}
      $(window).on('scroll', ()=>{
        if(!document.querySelector('header').classList.contains('no-scroll-change')){
        if(window.scrollY > getHeroBottomPercentage){
          $('header.custom').removeClass('opt-transparent opt-light');
          $('header.custom').removeClass('sf-page-header');
        } else {
          if(!$('.mobileWrapper').hasClass('isActive')) {
            $('header.custom').addClass('sf-page-header');
            {% if section.settings.is_transparent_sf %}
              $('header.custom').addClass('opt-transparent');
            {% endif %}
            {% if section.settings.is_light_sf %}
              $('header.custom').addClass('opt-light');
            {% endif %}
          }
        }
      }
      });
    {% endif %}
  });
</script>

